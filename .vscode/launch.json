    "version": "0.2.0",
    "configurations": [
        {
            "name": "vuejs: firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/ui/src",
            "pathMappings": [{ "url": "webpack:///src/", "path": "${webRoot}/" }]
          }
    ]
}